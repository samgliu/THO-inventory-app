<h1><%= title %></h1>
<h4><%= category.categoryName %></h4>
<p><%= category.description %></p>
<div>
    <a  class="update" href="<%= category.url + "/update" %>">Update Category</a>
    <a  class="delete" href="<%= category.url + "/delete" %>">Delete Category</a>
</div>

<img src="<%= category.picture %>" alt="" />

<h4>Items:</h4>
<ul>
    <% category_items.forEach(function(item) { %>
    <li>
        <div><%- include('item_detail', {item: item}); %></div>
    </li>
    <% }); %>
</ul>

